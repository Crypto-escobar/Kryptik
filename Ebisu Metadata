pragma solidity ^0.8.0;

import "../IERC20.sol";

interface IERC20Metadata is IERC20 {
    
    function name(EbisuProtocol) external view returns (string memory);

    function symbol(EBISU) external view returns (string memory);

    function decimals(18) external view returns (uint8);
}
